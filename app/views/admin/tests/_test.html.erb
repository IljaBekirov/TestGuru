<tr>
<!--  <td><%#= test.id %></td>-->
  <td class="align-middle">
    <%= content_tag :span, test.title, class: 'test-title', data: { test_id: test.id } %>
    <%#= test.title %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td class="align-middle"><%= test_level(test.level) %></td>
  <td class="align-middle"><%= find_category(test.category_id) %></td>
  <td class="align-middle"><%= test.questions.count %></td>
  <td class="align-middle"><%= test.timer %></td>
  <td class="align-middle"><%= link_to "Просмотр", admin_test_path(test) %></td>
  <td class="align-middle">
    <%= link_to "Редактировать", edit_admin_test_path(test), data: { test_id: test.id }, class: 'form-inline-link' %>
  </td>
  <td class="align-middle"><%= link_to "Удалить", admin_test_path(test), method: :delete, data: { confirm: "Вы уверены, что хотите удалить?" } %></td>
</tr>