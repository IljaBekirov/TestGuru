<tr>
  <td><%= test.id %></td>
  <td><%= test.title %></td>
  <td><%= test_level(test.level) %></td>
  <td><%= find_category(test.category_id) %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to "Просмотр", test %> |</td>
  <td><%= link_to "Редактировать", edit_test_path(test) %> |</td>
  <td><%= link_to "Удалить", test, method: :delete, data: { confirm: "Вы уверены, что хотите удалить?" } %> |</td>
  <td><%= button_to 'Старт', start_test_path(test), method: :post %></td>
</tr>